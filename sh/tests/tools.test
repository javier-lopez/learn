test X"$(compress --help 2>&1|head -1)"                      = X"Usage: compress [options] files ... file.[tar.gz,tar.bz2,zip...]"
test X"$(compress --cui 2>&1|head -1)"                       = X"Usage: compress [options] files ... file.[tar.gz,tar.bz2,zip...]"
