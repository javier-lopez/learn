printf "%s" "o@o/" > cuicui && printf "%s" "\oVo" > cuakcuak
compress -s cuicui cuicui.tar && test -f cuicui.tar
printf "%s" 'c.tar.gz' | compress cuicui cuakcuak && test -f c.tar.gz
