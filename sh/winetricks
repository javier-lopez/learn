    set_winver nt40
    if [ $WINETRICKS_QUIET ]
    then
        try $WINE "$WINETRICKS_CACHE"/mdac25/mdac_typ.exe /q /C:"setup /QNT"
    else
        try $WINE "$WINETRICKS_CACHE"/mdac25/mdac_typ.exe
    fi
    unset_winver
}

