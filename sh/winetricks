load_dsoundbug9612() {
    echo "Enabling DirectSound workaround for bug #9612"
    cat > "$WINETRICKS_TMP"/dsoundmaxshadow.reg <<_EOF_
REGEDIT4

[HKEY_CURRENT_USER\Software\Wine\DirectSound]
"MaxShadowSize"="0"

_EOF_
    try $WINE regedit "$WINETRICKS_TMP"/dsoundmaxshadow.reg
}

