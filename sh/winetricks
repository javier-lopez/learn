    try $WINE "$WINETRICKS_CACHE"/BZEditW32_1.6.5_Installer.exe $WINETRICKS_S
    try cp "$programfilesdir_unix/BZEdit1.6.5"/m*71* "$SYSTEM32_DLLS"
}

