load_dsound() {
    helper_directx_dl

    try_cabextract -d "$WINETRICKS_TMP" -L -F dxnt.cab "$WINETRICKS_CACHE"/$DIRECTX_NAME
    try_cabextract -d "$SYSTEM32_DLLS" -L -F 'dsound.dll' "$WINETRICKS_TMP/dxnt.cab"

    try $WINE regsvr32 dsound.dll

    override_dlls native dsound
}

